{
  "name": "micronative/entity-patcher",
  "description": "Doctrine entity patcher",
  "keywords": [
    "Doctrine",
    "ORM",
    "Entity",
    "Entity patcher"
  ],
  "homepage": "https://github.com/micronative/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ngodinhloc@gmail.com"
    }
  ],

  "require": {
    "php": "^7.4||^8.0",
    "doctrine/annotations": "^2.0",
    "doctrine/orm": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Micronative\\EntityPatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Samples\\": "samples/"
    }
  },
  "minimum-stability": "dev"
}
