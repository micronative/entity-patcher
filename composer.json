{
  "name": "micronative/entity-patcher",
  "description": "Doctrine entity patcher",
  "keywords": [
    "Doctrine",
    "ORM",
    "Entity",
    "Entity patcher"
  ],
  "homepage": "https://github.com/micronative/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ngodinhloc@gmail.com"
    }
  ],

  "require": {
    "php": "^7.4||^8.0",
    "doctrine/annotations": "^1.6",
    "psr/log": "^1.1",
    "psr/cache": "^1.0",
    "symfony/event-dispatcher": "^2.8",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.3.0",
    "monolog/monolog": "^2.1.1"
  },
  "autoload": {
    "psr-4": {
      "Bigcommerce\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Samples\\": "samples/"
    }
  },
  "minimum-stability": "dev"
}
